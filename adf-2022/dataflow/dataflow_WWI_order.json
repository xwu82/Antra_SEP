				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          OrderID,",
				"          CustomerID,",
				"          CustomerName,",
				"          PhoneNumber,",
				"          Description,",
				"          Quantity,",
				"          UnitPrice,",
				"          TaxRate,",
				"          OrderDate,",
				"          ExpectedDeliveryDate,",
				"          CustomerPurchaseOrderNumber,",
				"          DeliveryAddressLine1,",
